{
  "storeVersion": 1,
  "docType": "QuickTemplate",
  "docTypeVersion": 1,
  "docId": "operation-kubectlLogs",
  "content": {
    "index": "kubectl logs",
    "name": "Kubectl, pod logs",
    "templates": [
      {
        "id": "input",
        "textTemplate": "kubectl logs ${podName}${namespace}${tail}${reverse}${toFile}",
        "defaultText": null
      },
      {
        "id": "namespace",
        "textTemplate": " -n${kubeNamespace:KubeNamespace}",
        "defaultText": ""
      },
      {
        "id": "tail",
        "textTemplate": " | tail -n 30",
        "defaultText": ""
      },
      {
        "id": "reverse",
        "textTemplate": " | tac",
        "defaultText": ""
      },
      {
        "id": "toFile",
        "textTemplate": " > \"${podName}.log\"",
        "defaultText": ""
      }
    ]
  }
}